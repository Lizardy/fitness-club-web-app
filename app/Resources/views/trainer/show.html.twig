{% extends 'base.html.twig' %}

{% block body %}

    {% include ':default:admin_nav.html.twig' with {'pill': 'trainers', 'header_text': 'Trainer info'} %}

    <table>
        <tbody>
            <tr>
                <th>Lastname</th>
                <td>{{ trainer.lastName }}</td>
            </tr>
            <tr>
                <th>Firstname</th>
                <td>{{ trainer.firstName }}</td>
            </tr>
            <tr>
                <th>Patronym</th>
                <td>{{ trainer.patronym }}</td>
            </tr>
        </tbody>
    </table>

    <div>
        <a href="{{ path('admin_trainers_index') }}" class="btn btn-default" role="button">Back to the list</a>
    </div>

{% endblock %}
